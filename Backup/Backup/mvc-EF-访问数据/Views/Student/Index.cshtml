}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            StuNO
        </th>
        <th>
            StuName
        </th>
        <th>
            StuPhoto
        </th>
        <th>
            StuBirthday
        </th>
        <th>
            StuAddress
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
